{
  "name": "splittypie",
  "version": "0.0.0",
  "description": "Split your expenses with ease",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "deploy": "rm -rf dist/ && ember build && firebase use dev && firebase deploy",
    "deploy:prod": "rm -rf dist/ && ember build --environment=production && cp CNAME dist/ && cp dist/index.html dist/200.html && surge dist/",
    "deploy:staging": "rm -rf dist/ && ember build --environment=staging && firebase use staging && firebase deploy",
    "deploy:offline": "rm -rf dist/ && ember build --environment=offline && firebase use offline && firebase deploy",
    "seed": "firebase use dev && firebase database:set / firebase_seed.json",
    "seed:staging": "firebase use staging && firebase database:set / firebase_seed.json"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.7.1",
    "broccoli-asset-rev": "^2.2.0",
    "broccoli-serviceworker": "0.1.0",
    "dotenv": "^2.0.0",
    "ember-ajax": "2.0.0-beta.2",
    "ember-cli": "2.4.2",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-babel": "^5.1.5",
    "ember-cli-content-security-policy": "0.5.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-eslint": "^1.4.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-ic-ajax": "0.2.4",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-inline-content": "0.4.0",
    "ember-cli-moment-shim": "1.1.0",
    "ember-cli-qunit": "^2.0.2",
    "ember-cli-release": "1.0.0-beta.1",
    "ember-cli-sass": "5.3.0",
    "ember-cli-sri": "^2.0.0",
    "ember-cli-test-model-waiter": "0.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "2.4.0",
    "ember-data-fixture-adapter": "git://github.com/emberjs/ember-data-fixture-adapter#2-0",
    "ember-disable-proxy-controllers": "^1.0.1",
